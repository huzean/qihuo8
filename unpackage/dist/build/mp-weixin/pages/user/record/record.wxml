<view class="record"><view class="head"><view class="head_l"><view class="head_l_1">余额（元）</view><view class="head_l_2">{{commissionCount}}</view></view><view data-event-opts="{{[['tap',[['jump_cash',['$event']]]]]}}" class="head_r" bindtap="__e">提现</view></view><block wx:for="{{cash_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="list"><view class="list_01"><block wx:if="{{item.extract_type=='bank'}}"><view class="list_01_l">银行卡提现</view></block><view class="list_01_r">{{item.extract_price}}</view></view><view class="list_02"><view class="list_01_l">{{item.add_time}}</view><block wx:if="{{item.status==0}}"><view class="list_01_r">审核中</view></block><block wx:if="{{item.status==1}}"><view class="list_01_r">提现成功</view></block><block wx:if="{{item.status==-1}}"><view class="list_01_r">提现失败</view></block></view></view></block></block></view>