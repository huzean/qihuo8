<view class="box"><view class="enquirt_box"><view class="nav"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['lun_bo',[index]]]]]}}" class="{{['nav_item',index==navactive?'active':'']}}" bindtap="__e">{{''+item.name+''}}</view></block></view></view><block wx:if="{{navactive==0}}"><view class="plan1"><block wx:for="{{enquirtlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="enquirt_item"><view class="item_image"><view class="imgs"></view><view>{{item.product}}</view><view>{{item.proceed}}</view></view><view class="item_time"><text>下单时间:</text><text>{{item.time}}</text></view><view class="item_quantity"><text>采购数量</text><text>{{item.quantity}}</text><view class="itwm_price"><view>结束时间<tui-countdown class="box" vue-id="{{'71f08aea-1-'+index}}" time="{{timeList[1]}}" bind:__l="__l"></tui-countdown></view><text>￥<text>{{item.price}}</text></text></view></view></view></block></view></block><block wx:if="{{navactive==1}}"><view class="plan1"><block wx:for="{{locationlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="msg_box"><view class="location"><text class="location_item">{{item.location}}</text><text class="prage_item">{{item.prage+"元"}}</text><text class="grade_item"><text class="grade">评分:</text>{{''+item.grade+''}}</text><view class="handclick"><text data-event-opts="{{[['tap',[['sub',['$event']]]]]}}" bindtap="__e">开牌</text></view></view><view hidden="{{!(isShow)}}" class="hacking"><view data-event-opts="{{[['tap',[['handleShut',['$event']]]]]}}" class="shut" bindtap="__e">X</view></view></view></block><view class="hint">开牌需要消耗15个币</view></view></block></view>