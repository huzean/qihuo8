}
.nicheng .tan_nc .nc_in {
  padding: 30px 20px;
}
.nicheng .tan_nc .nc_btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 14px;
  border-top: 1px solid #F4F4F4;
}
.nicheng .tan_nc .nc_btn .btn_l {
  width: 50%;
  text-align: center;
  padding: 15px 0;
