.list-cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: baseline;
  -webkit-align-items: baseline;
          align-items: baseline;
  padding: 20rpx 30rpx;
  line-height: 60rpx;
  position: relative;
  background: #fff;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.list-cell.log-out-btn {
  margin-top: 40rpx;
}
.list-cell.log-out-btn .cell-tit {
  color: #fa436a;
  text-align: center;
  margin-right: 0;
}
.list-cell.cell-hover {
  background: #fafafa;
}
.list-cell.b-b:after {
  left: 30rpx;
}
.list-cell.m-t {
  margin-top: 16rpx;
}
.list-cell .cell-more {
  -webkit-align-self: baseline;
          align-self: baseline;
