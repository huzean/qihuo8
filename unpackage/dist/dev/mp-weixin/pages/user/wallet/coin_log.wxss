}
.tui-loading-3.tui-loading-green {
	border-color: #ededed #ededed #ededed #ed3f14;
}
@-webkit-keyframes tui-rotate {
0% {
		-webkit-transform: rotate(0);
		        transform: rotate(0);
}
100% {
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}
@keyframes tui-rotate {
0% {
		-webkit-transform: rotate(0);
		        transform: rotate(0);
}
100% {
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}
