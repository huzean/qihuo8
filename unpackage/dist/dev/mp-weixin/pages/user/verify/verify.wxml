<view class="content"><view class="agent"><view class="head">{{'认证状态:'+verify_status+''}}</view></view><view class="row b-b"><text class="tit">姓名</text><input class="input" disabled="{{can_not_edit}}" type="text" placeholder="请填写真实姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','cname','$event',[]],['form']]]]]}}" value="{{form.cname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">联系手机</text><input class="input" type="number" disabled="{{can_not_edit}}" placeholder="请填写手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','cphone','$event',[]],['form']]]]]}}" value="{{form.cphone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">公司名称</text><input class="input" type="text" disabled="{{can_not_edit}}" placeholder="请填写企业名称" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','ccorpname','$event',[]],['form']]]]]}}" value="{{form.ccorpname}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地区</text><view data-event-opts="{{[['tap',[['showRegion',['$event']]]]]}}" class="result" bindtap="__e">{{resultInfo.result}}</view><w-picker class="vue-ref" vue-id="38747742-1" mode="region" defaultVal="{{['广东省','广州市','荔湾区']}}" data-ref="region" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" disabled="{{can_not_edit}}" placeholder="请填写详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','caddress','$event',[]],['form']]]]]}}" value="{{form.caddress}}" bindinput="__e"/></view><view class="btn"><button data-event-opts="{{[['tap',[['sub_verify',['$event']]]]]}}" class="add-btn" bindtap="__e">提交认证</button></view></view>