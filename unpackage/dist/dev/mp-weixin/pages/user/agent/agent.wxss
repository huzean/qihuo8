.agent .none {
  padding: 150px 0;
  text-align: center;
  color: #00A9F4;
  line-height: 50px;
}
.agent .none ._img {
  width: 60px;
  height: 60px;
}
.agent .head {
  background-color: #E0451D;
  color: #fff;
  text-align: center;
  padding: 40px 10px 30px;
}
.agent .head ._span {
  font-size: 30px;
  padding: 0 5px;
  font-size: 18px;
}
.agent .t_tou {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 10px;
  height: 40px;
  line-height: 30px;
}
.agent .tou_1 {
  width: 20%;
  text-align: center;
  font-size: 14px;
  font-weight: bold;
}
.agent .tou_2 {
  width: 50%;
  text-align: center;
  font-size: 14px;
  font-weight: bold;
}
.agent .tou_3 {
  width: 30%;
  text-align: center;
  font-size: 14px;
  font-weight: bold;
}
.agent .t_01 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 50px;
  line-height: 50px;
}
.agent .t_01_1 {
  width: 34%;
  text-align: center;
  font-size: 14px;
}
.agent .t_01_1 ._img {
  margin-top: 8px;
  width: 35px;
  height: 35px;
  -webkit-border-radius: 35px;
          border-radius: 35px;
}
.agent .t ._li:nth-of-type(odd) {
  background-color: #EEEEEE;
}
.agent .t ._li:nth-of-type(even) {
  background-color: #fff;
}
.agent .head_r {
  background-color: #fff;
  color: #FD6766;
  height: 30px;
  line-height: 30px;
  width: 60px;
  text-align: center;
  -webkit-border-radius: 10px;
          border-radius: 10px;
  margin-top: 15px;
  margin-left: 30px;
}

