dex"><view class="middle_box_top_item" data-key="{{item.name}}" data-event-opts="{{[['tap',[['clicknavi',[index]]]]]}}" bindtap="__e"><image class="tui-category-img" src="{{'/static/images/mall/nav/'+item.img}}" mode="scaleToFill"></image><view class="tui-category-name">{{item.name}}</view></view></block></view></view><block wx:for="{{productlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="content_list"><view class="list_left"><image class="tui-category-img" src="{{'/static/images/mall/nav/'+item.img}}" mode="scaleToFill"></image><view class="list_quantity"><view class="list_text"><view><text>{{item.amount}}</text></view><view><text>家</text>供应商报价</view></view><image src="../../static/images/mall/nav/qgou.png" mode="widthFix"></image></view></view><view class="list_right"><view class="right_box"><view class="right_text_noe"><view>{{item.name}}</view><view><text class="text_quantity">{{item.number}}</text><text class="text">件</text></view></view><view class="specification">{{item.specification}}</view><view class="time time_delivery"><text>交货时间:</text><text>{{item.delivery}}</text></view><view class="time time_end"><text>截止时间:</text><text>{{item.expirationdate}}</text></view><view class="location"><view><text class="bg iconfont icon-weizhi"></text><text class="location_text bg">{{item.location}}</text></view><view class="make"><view data-event-opts="{{[['tap',[['navto',['$event']]]]]}}" bindtap="__e">报价</view><text class="line_segment"></text><view class="make_right"><text>{{item.integral}}</text><text class="iconfont icon-jifen"></text></view></view><view data-event-opts="{{[['tap',[['showImg',[index]]]]]}}" bindtap="__e"><block wx:if="{{!showSearch}}"><view class="tui-collection"><text class="iconfont icon-shoucang1"></text><view class="enshrine">收藏</view></view></block><block wx:if="{{showSearch}}"><view class="tui-collection"><image src="../../static/images/mall/nav/07.png" mode="widthFix"></image><view class="enshrine2">收藏</view></view>