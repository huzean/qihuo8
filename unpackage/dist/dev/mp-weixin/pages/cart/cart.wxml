<view class="cart"><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="k" wx:key="id"><block><view class="cart-item"><view class="image-wrappers"><image src="{{item.product_attr_unique==''?item.productInfo.image:item.productInfo.attrInfo.image}}" data-event-opts="{{[['tap',[['jump_detail',['$0'],[[['cartList','id',item.id,'product_id']]]]]]]}}" bindtap="__e" class="_img"></image><view data-event-opts="{{[['tap',[['_select_item',['item',k]]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox',(item.radio)?'checked':'']}}" bindtap="__e"></view></view><view class="item-right"><text data-event-opts="{{[['tap',[['jump_detail',['$0'],[[['cartList','id',item.id,'product_id']]]]]]]}}" class="clamp title" bindtap="__e">{{item.productInfo.store_name}}</text><text class="attr">{{item.productInfo.attrInfo.suk}}</text><text class="price">{{"¥"+item.truePrice}}</text><tui-numberbox vue-id="{{'1775f8c0-1-'+k}}" max="{{item.trueStock}}" min="{{1}}" value="{{item.cart_num}}" data-event-opts="{{[['^change',[['numberChange',['$event',k,'$0'],[[['cartList','id',item.id,'id']]]]]]]}}" bind:change="__e" bind:__l="__l"></tui-numberbox></view><text data-event-opts="{{[['tap',[['deleteCartItem',[k]]]]]}}" class="del-btn yticon icon-fork" bindtap="__e"></text></view></block></block></view><view class="action-section" style="{{'bottom:'+(is_lhp?'130rpx':'99rpx')+';'}}"><view class="checkbox"><block wx:if="{{cartList}}"><view data-event-opts="{{[['tap',[['choose_all',['$event']]]]]}}" class="all-btn" bindtap="__e">全选</view></block><block wx:if="{{cartList}}"><view data-event-opts="{{[['tap',[['clearCart',['$event']]]]]}}" class="clear-btn" bindtap="__e">清空</view></block></view><view class="total-box"><text class="price">{{"¥"+total}}</text></view><button class="no-border confirm-btn" type="primary" data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" bindtap="__e">去结算</button></view></view>